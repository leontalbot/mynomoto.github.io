(ns hoplon.mui
  (:require
    [cljsjs.mui]))

(defelem appbar [_ kids]
  (div :class "mui-appbar"
    (div :class "mui-container"
      (table :width "100%"
        :class "app-bar-inner"
        (apply (tr
                 :class "mui-appbar-height"
                 :css {:vertical-align "middle"}) kids)))))

(defelem panel [_ kids]
  (apply (div :class "mui-panel") kids))
